
public class Trader {
	private final String name;
	private final String city;
	
	private Trader(String n, String c) {
		this.name = n;
		this.city = c;
	}

	public String getName() {
		return name;
	}

	public String getCity() {
		return city;
	}

	@Override
	public String toString() {
		return "Trader: " + this.name + " in " + this.city + "";
	}
}
